{
  "name": "@goat-bravos/intern-hub-layout",
  "version": "1.0.2",
  "description": "Angular library for intern hub layout components",
  "sideEffects": false,
  "scripts": {
    "ng": "ng",
    "build": "ng build --configuration production",
    "build:dev": "ng build --configuration development",
    "watch": "ng build --configuration development --watch",
    "test": "ng test",
    "publish:lib": "npm run build && npm publish dist/intern-hub-layout --access public"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": false,
  "packageManager": "npm@10.9.2",
  "peerDependencies": {
    "@angular/common": "^21.0.0",
    "@angular/core": "^21.0.0",
    "@angular/router": "^21.0.0",
    "dynamic-ds": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "dynamic-ds": {
      "optional": true
    }
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular/build": "^21.1.1",
    "@angular/cli": "^21.1.1",
    "@angular/common": "^21.1.0",
    "@angular/compiler": "^21.1.0",
    "@angular/compiler-cli": "^21.1.0",
    "@angular/core": "^21.1.0",
    "@angular/forms": "^21.1.0",
    "@angular/platform-browser": "^21.1.0",
    "@angular/router": "^21.1.0",
    "dynamic-ds": "^1.0.2",
    "jsdom": "^27.1.0",
    "ng-packagr": "^21.0.0",
    "rxjs": "~7.8.0",
    "typescript": "~5.9.2",
    "vitest": "^4.0.8"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "angular",
    "library",
    "layout",
    "components"
  ],
  "license": "MIT"
}
